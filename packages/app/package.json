{
	"name": "@kobayashik-faber_github_io/app",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"prepare": "svelte-kit sync && panda codegen",
		"dev": "vite dev --host",
		"build": "vite build",
		"preview": "vite preview --host",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "eslint .",
		"test:unit": "vitest",
		"test": "npm run test:unit -- --run"
	},
	"devDependencies": {
		"@eslint/compat": "^1.2.3",
		"@fontsource-variable/noto-sans-jp": "5.1.1",
		"@fontsource/oswald": "5.1.1",
		"@pandacss/dev": "0.51.1",
		"@sveltejs/adapter-static": "^3.0.8",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^4.0.0",
		"eslint": "^9.7.0",
		"eslint-plugin-svelte": "^2.36.0",
		"globals": "^15.0.0",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"typescript": "^5.0.0",
		"typescript-eslint": "^8.0.0",
		"vite": "^5.4.11",
		"vitest": "^2.0.4"
	}
}
